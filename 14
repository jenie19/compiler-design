#include <stdio.h>

int main() {
    int a = 5, b = 10, c = 15;
    int result1, result2;

    // Common subexpression
    int common = a * b + c;

    result1 = common; // First use of common subexpression
    result2 = common; // Second use of common subexpression

    printf("Result 1: %d\n", result1);
    printf("Result 2: %d\n", result2);

    return 0;
}
